class Rectangle {

	var length = 5;
	var width = 5;

	
	define area() { return length * width; }

}

define main() {
	var x = Rectangle();
	var xArea = x.area;
	x.length = 10;
	x.width = 20;


	var y = Rectangle();
	var yArea = y.area;
	y.length = 40;
	y.width = 100;

	println(xArea());
	println(yArea());

}
