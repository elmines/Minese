define main() {
	define double(var a)
		{
		return a * 2;
		}

	var values = [2, "Ethan", 5, double, 11];
	println("values         = ", values);
	println("len(values)    = ", len(values));
	println("get(values, 4) = ", get(values, 4));

	println("Setting index 1 to Jerry...");
	set(values, 1, "Jerry");
	println("values         = ", values);
	println("Appending 42...");
	append(values, 42);
	println("values         = ", values);

}
